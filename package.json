{
  "name": "@myadbox/exolite",
  "private": true,
  "version": "0.0.12",
  "type": "module",
  "author": "Sesimi R&D",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "deploy",
    "setup": "npm install && node dev/rename.mjs && npm run update-eslint",
    "update-eslint": "npx install-peerdeps --dev @myadbox/eslint-config-exoplanet",
    "update-name": "node dev/rename.mjs --force"
  },
  "engines": {
    "node": ">=16.0"
  },
  "repository": {
    "url": "https://github.com/myadbox/exolite",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/myadbox/exolite/issues"
  },
  "dependencies": {
    "@myadbox/exoplanet-toolkit": "^1.0.19",
    "lazysizes": "^5.3.2"
  },
  "devDependencies": {
    "@myadbox/eslint-config-exoplanet": "^1.0.7",
    "@myadbox/exoplanet-deployment": "^0.1.15",
    "@myadbox/nebula-template-utils": "^0.0.87",
    "@myadbox/prettier-config-exoplanet": "^1.0.5",
    "@types/node": "^18.7.6",
    "@types/react": "^17.0.39",
    "@vitejs/plugin-react": "^2.0.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace": "^1.2.1",
    "typescript": "^4.8.3",
    "vite": "^3.0.6"
  },
  "prettier": "@myadbox/prettier-config-exoplanet",
  "eslintConfig": {
    "root": true,
    "extends": "@myadbox/eslint-config-exoplanet"
  },
  "eslintIgnore": [
    "src/animation/lib/**/*",
    "node_modules",
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
